{
  "presets":[
    "@babel/preset-react",
    [ "@babel/preset-env", {
      "targets": {
        "browsers": "> 2%"
      }
    }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "styled-components"
  ]
}
